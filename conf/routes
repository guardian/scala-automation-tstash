# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET         /                                 controllers.MainController.index()
GET         /set                              controllers.MainController.set(setName, setDate)
GET         /test                             controllers.MainController.test(testName, testDate, setName, setDate)
GET         /report                           controllers.MessageController.report(testName, testDate, setName, setDate)
#GET         /assets/javascripts/routes        controllers.MessageController.javascriptRoutes()

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                     controllers.Assets.at(path="/public", file)
GET         /webjars/*file                    controllers.WebJarAssets.at(file)
